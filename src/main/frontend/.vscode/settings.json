{
  "eslint.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "travis.pro": true
}
